{
  "name": "e-job-server",
  "version": "1.0.0",
  "private": true,
  "main": "src/index.ts",
  "scripts": {
    "dev": "ts-node-dev --no-notify --respawn --transpile-only -r dotenv/config src/index.ts",
    "start": "ts-node -r dotenv/config src/index.ts",
    "generate:prisma": "prisma2 generate",
    "generate:nexus": "cross-env NODE_ENV=development ts-node --transpile-only -r dotenv/config src/gen-schema.ts",
    "generate": "npm run generate:prisma && npm run generate:nexus",
    "lint": "eslint src/**/*.ts",
    "lintfix": "eslint --fix src/**/*.ts"
  },
  "dependencies": {
    "@prisma/photon": "^2.0.0-preview018.2",
    "apollo-server": "^2.9.13",
    "argon2": "^0.25.1",
    "cross-env": "^6.0.3",
    "dotenv": "^8.2.0",
    "graphql": "^14.5.8",
    "graphql-middleware": "^4.0.2",
    "graphql-shield": "^7.0.5",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "mailgun-js": "^0.22.0",
    "nexus": "^0.12.0-rc.5",
    "nexus-prisma": "^0.6.1",
    "pluralize": "^8.0.0"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^8.3.5",
    "@types/lodash": "^4.14.149",
    "@types/mailgun-js": "^0.22.3",
    "@types/pluralize": "0.0.29",
    "@typescript-eslint/eslint-plugin": "^2.11.0",
    "@typescript-eslint/parser": "^2.11.0",
    "eslint": "^6.6.0",
    "eslint-config-standard-with-typescript": "^11.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^9.2.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "ts-node": "^8.5.4",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "^3.7.3"
  }
}
