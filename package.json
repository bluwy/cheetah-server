{
  "name": "cheetah-server",
  "version": "1.0.0",
  "private": true,
  "main": "dist/index.js",
  "author": "Bjorn Lu",
  "license": "MIT",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "tsnd --no-notify --respawn -T -r dotenv/config src/index.ts",
    "generate": "ts-node -T -r dotenv/config src/schema.ts",
    "db:setup": "ts-node -T -r dotenv/config scripts/db-setup.ts",
    "db:clean": "ts-node -T -r dotenv/config scripts/db-clean.ts",
    "rd:clean": "ts-node -T -r dotenv/config scripts/rd-clean.ts",
    "build": "tsc --outDir dist",
    "lint": "eslint ./**/*.ts",
    "lint:fix": "eslint --fix ./**/*.ts",
    "test": "jest"
  },
  "dependencies": {
    "@nexus/schema": "^0.15.0",
    "@sendgrid/mail": "^7.2.3",
    "@types/graphql-iso-date": "3.3.3",
    "apollo-server-express": "^2.16.1",
    "argon2": "^0.27.0",
    "cookie-parser": "^1.4.5",
    "dataloader": "^2.0.0",
    "express": "^4.17.1",
    "graphql": "^14.7.0",
    "graphql-iso-date": "^3.6.1",
    "ioredis": "^4.17.3",
    "knex": "^0.21.4",
    "lodash": "^4.17.20",
    "nanoid": "^3.1.12",
    "objection": "^2.2.2",
    "pg": "^8.3.0",
    "uuid": "^8.3.0"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.7",
    "@types/ioredis": "^4.17.3",
    "@types/jest": "^26.0.10",
    "@types/lodash": "^4.14.159",
    "@types/node": "^14.0.27",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^3.9.0",
    "@typescript-eslint/parser": "^3.9.0",
    "apollo-server-testing": "^2.16.1",
    "dotenv": "^8.2.0",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "ioredis-mock": "^4.21.2",
    "jest": "^26.4.0",
    "prettier": "^2.0.5",
    "ts-jest": "^26.2.0",
    "ts-node": "^8.10.2",
    "ts-node-dev": "^1.0.0-pre.57",
    "typescript": "^3.9.7"
  }
}
